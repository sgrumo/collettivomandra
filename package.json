    "axios": "^0.27.2",
    "localstorage-slim": "^2.3.0",
    "next": "^12.3.0",
    "openapi-typescript": "^5.4.1",
    "openapi-typescript-codegen": "^0.23.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.11",
    "eslint": "^8.23.1",
    "eslint-config-next": "^12.3.0",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.3"
  }
